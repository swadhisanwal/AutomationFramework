<?xml version ='1.0' encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name= "The Economist Web Application Test Automation Suite" thread-count="1" parallel="methods">

 <listeners>
        <listener 
        class-name="com.economist.qa.ExtentReportListener.ExtentReporter"/>
  </listeners>

<test name="Economist WebApp Test Cases">
     <classes>
       <class name="com.economist.qa.testcases.HomeTest"/>
       <class name="com.economist.qa.testcases.BrowseJobTest"/>
        <class name="com.economist.qa.testcases.SelectJobTest"/>
       <class name="com.economist.qa.testcases.LoginPageTest"/>
       <class name="com.economist.qa.testcases.RegisterPageTest"/>
      
     </classes>
   </test>
</suite>